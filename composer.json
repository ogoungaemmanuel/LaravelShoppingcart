{
    "name": "xslaincart/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ogounga Emmanuel",
            "email": "info@xslain.com"
        }
    ],
    "require": {
      	"illuminate/support": "8|^9|^10|^11",
    	"illuminate/session": "8|^9|^10|^11",
       	"illuminate/events": "8|^9|^10|^11"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0 || ~10.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Xslaincart\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xslaincart\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "xslaincartcart/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Xslaincart\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Xslaincart\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
