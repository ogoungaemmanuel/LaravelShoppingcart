{
    "name": "xslaincart/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ogounga Emmanuel",
            "email": "info@xslain.com"
        }
    ],
    "require": {
        "php": "^8.2",
      	"illuminate/support": "^12.0",
    	"illuminate/session": "^12.0",
       	"illuminate/events": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3|^12.0.1",
        "mockery/mockery": "^1.6.10 ",
        "orchestra/testbench": "^10.2.2",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Xslaincart\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xslaincart\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "xslaincartcart/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Xslaincart\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Xslaincart\\Shoppingcart\\Facades\\Cart",
                "Booking": "Xslaincart\\Shoppingcart\\Facades\\Booking",
                "Expense": "Xslaincart\\Shoppingcart\\Facades\\Expense",
                "Invoice": "Xslaincart\\Shoppingcart\\Facades\\Invoice",
                "Quotation": "Xslaincart\\Shoppingcart\\Facades\\Quotation",
                "Fee": "Xslaincart\\Shoppingcart\\Facades\\Fee"
            }
        }
    }
}
